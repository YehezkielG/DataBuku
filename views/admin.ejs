<div class="my-5 px-10">
    <h1 class="text-2xl">
        Selamat datang admin
    </h1>
    <hr class="my-7">
    <div class="flex justify-between">
        <h1 class="font-bold text-2xl">
            Data Buku
        </h1>
        <div>
            <p class="flex items-center">
                <span class="material-symbols-outlined p-1 bg-red-500 text-white rounded-full text-sm mx-1 my-1" style="font-size:17px;">
                    delete
                </span>
                Hapus Data
            </p>
            <p class="flex items-center">
                <span class="material-symbols-outlined p-1 bg-blue-500 text-white rounded-full text-sm mx-1 my-1" style="font-size:17px;">
                    edit
                </span>
                Edit Data
            </p>
        </div>
    </div>
    <div>
        <% databuku.forEach(buku => { %>
                <div class="shadow-lg flex p-2">
                    <div>
                        <img src="<%= buku.sampul %>" alt="">
                        
                    </div>
                    <div>

                    </div>
                </div>
        <% }) %>
    </div>
</div>